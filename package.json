{
  "name": "start-pub",
  "version": "1.0.0",
  "scripts": {
    "prestart": "babel-node buildScripts/preStart.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "share": "npm-run-all --parallel open:src localtunnel",
    "localtunnel": "lt --port 3000",
    "start-parallel": "npm-run-all --parallel security-check open:src",
    "security-check": "nsc check",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/**.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Artur Nizamov",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "^4.0.2",
    "express": "^4.16.4",
    "jsdom": "^13.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "path": "^0.12.7",
    "style-loader": "^0.23.1",
    "webpack": "^4.25.1",
    "webpack-dev-middleware": "^3.4.0",
    "whatwg-fetch": "1.0.0"
  }
}
